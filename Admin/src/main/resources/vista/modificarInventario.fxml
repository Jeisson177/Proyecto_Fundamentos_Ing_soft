<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.modificarInventario">
    <stylesheets>

    </stylesheets>
    <children>
        <AnchorPane prefHeight="74.0" prefWidth="600.0" style="-fx-background-color: EFB810;">
         <children>
            <Text fill="#f7f5f5" layoutX="14.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Bella Ventura">
               <font>
                  <Font size="21.0" />
               </font>
            </Text>
            <ImageView fx:id="instaImagen" fitHeight="52.0" fitWidth="60.0" layoutX="517.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="selecHome" layoutX="213.0" layoutY="21.0" mnemonicParsing="false" onAction="#IrHome" style="-fx-background-color: transpare;" text="Home" textFill="WHITE" underline="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="selecMenu" layoutX="327.0" layoutY="21.0" mnemonicParsing="false" onAction="#IrMenu" style="-fx-background-color: transpare;" text="Menu" textFill="WHITE" underline="true">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children></AnchorPane>
        <TableView fx:id="tablaInventario" layoutX="55.0" layoutY="128.0" prefHeight="226.0" prefWidth="410.0" style="-fx-text-fill: black;">
