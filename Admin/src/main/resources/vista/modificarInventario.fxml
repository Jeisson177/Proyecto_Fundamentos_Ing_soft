                <TableColumn fx:id="columnaNombre" prefWidth="100.0" text="Nombre" />
                <TableColumn fx:id="columnaCantidad" prefWidth="100.0" text="Cantidad" />
                <TableColumn fx:id="columnaFecha" prefWidth="100.0" text="Fecha Vencimiento" />
                <TableColumn fx:id="columnaUnidad" prefWidth="100.0" text="Unidad" />
            </columns>
        </TableView>
      <Text fill="#240cd9" layoutX="25.0" layoutY="107.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Inventario Ingredientes">
         <font>
            <Font size="23.0" />
         </font>
      </Text>
      <Button fx:id="selecAgregar" layoutX="495.0" layoutY="174.0" mnemonicParsing="false" onAction="#IrModificarInventario" prefHeight="52.0" prefWidth="71.0" text="Agregar" />
      <Button fx:id="selecEliminar" layoutX="495.0" layoutY="253.0" mnemonicParsing="false" onAction="#IrModificarInventario" prefHeight="52.0" prefWidth="71.0" text="Eliminar" />
      <Button fx:id="selecInicio" layoutX="373.0" layoutY="87.0" mnemonicParsing="false" onAction="#IrInicio" style="-fx-background-color: black;" text="Volver a Inicio" textFill="WHITE" />
    </children>
